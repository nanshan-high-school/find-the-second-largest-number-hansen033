#include <iostream>
using namespace std;

int main() {
  long long x, f = -9223372036854775808, s = -9223372036854775808;
  unsigned long long a;
  cout << "你的陣列有幾個項?";
  cin >> a;
  for ( int t = 0 ; t < a ; t ++ ){
    cout << "輸入第" << t + 1 << "個數";
    cin >> x;
    if ( x > f ){
      s = f;
      f = x;
    } else if (x > s){
      s = x;
    }
  }
  cout << "\n第二大的數是" << s;
}
